<div class="row-fluid">
    <div class="span12">
		<form>
			<div class="formSep">
				<label>Username</label>
				<input type="text">
			</div>
			<div class="formSep">
				<label>Password</label>
				<input type="password">
			</div>
			<div class="formSep">
				<div class="row-fluid">
					<div class="span6">
						<label>Textarea</label>
						<textarea name="txtarea_reg" id="txtarea_reg" cols="10" rows="3"></textarea>
					</div>
					<div class="span6">
						<label>Textarea with <code>.span8</code> class</label>
						<textarea name="txtarea_sp" id="txtarea_sp" cols="10" rows="3" class="span8"></textarea>
					</div>
				</div>
			</div>
			<div class="formSep">
				<div class="row-fluid">
					<div class="span6">
						<label>Regular select</label>
						<select>
							<option>1</option>
							<option>2</option>
							<option>3</option>
							<option>4</option>
							<option>5</option>
						</select>
					</div>
					<div class="span6">
						<label>Select with <code>.span8</code> class</label>
						<select class="span8">
							<option>1</option>
							<option>2</option>
							<option>3</option>
							<option>4</option>
							<option selected="selected">5</option>
						</select>
					</div>
				</div>
				<div class="row-fluid">
					<div class="span6">
						<label>Multiselect</label>
						<select multiple="multiple">
							<option>1</option>
							<option>2</option>
							<option>3</option>
							<option>4</option>
							<option>5</option>
						</select> 
					</div>
					<div class="span6">
						<label>Multiselect with <code>.span8</code> class</label>
						<select multiple="multiple" class="span8">
							<option>1</option>
							<option>2</option>
							<option>3</option>
							<option>4</option>
							<option>5</option>
						</select> 
					</div>
				</div>
			</div>
			<div class="formSep">
				<div class="row-fluid">
					<div class="span6">
						<label>Checkboxes</label>
						<label class="checkbox">
							<input type="checkbox" value="option1" name="optionsCheckboxList1">
							Lorem ipsum dolor sit amet, consectetur
						</label>
						<label class="checkbox">
							<input type="checkbox" value="option2" name="optionsCheckboxList2">
							Lorem ipsum dolor sit amet, consectetur adipiscing elit
						</label>
						<label class="checkbox">
							<input type="checkbox" value="option3" name="optionsCheckboxList3">
							Lorem ipsum dolor sit amet
						</label>
					</div>
					<div class="span6">
						<label>Radio buttons</label>
						<label class="radio">
							<input type="radio" checked="" value="option1" id="optionsRadios1" name="optionsRadios">
							Lorem ipsum dolor sit amet, consectetur
						</label>
						<label class="radio">
							<input type="radio" value="option2" id="optionsRadios2" name="optionsRadios">
							Lorem ipsum dolor sit amet, consectetur adipiscing elit
						</label>
					</div>
				</div>
			</div>
			<div class="formSep">
				<p class="sepH_c">Prepended an appended inputs</p>
				<div class="row-fluid">
					<div class="span4">
						<div class="input-prepend">
							<span class="add-on"><i class="icon-envelope"></i></span><input type="text" size="16" class="span5">
						</div>  
					</div>
					<div class="span4">
						<div class="input-prepend">
							<span class="add-on">(+256)</span><input type="text" size="16" class="span5">
						</div>  
					</div>
					<div class="span4">
						<div class="input-append">
							<input type="text" size="16" class="span5"><button type="button" class="btn">Change</button><button type="button" class="btn">Ok</button>
						</div> 
					</div>
				</div>
				<div class="row-fluid">
					<div class="span4">
						<div class="input-append">
							<input type="text" size="16" class="span5"><span class="add-on">.00</span>
						</div>
					</div>
					<div class="span4">
						<div class="input-prepend input-append">
							<span class="add-on">$</span><input type="text" size="16" class="span5"><span class="add-on">.00</span>
						</div>
					</div>
					<div class="span4">
						<div class="input-append">
							<input type="text" size="16" class="span5"><button type="button" class="btn">OK</button>
						</div> 
					</div>
				</div>
			</div>
			<div class="formSep">
				<p class="sepH_c">Form control states</p>
				<div class="row-fluid">
					<div class="span3">
						<label>Focus state</label>
						<input type="text" class="focused span10">
					</div>
					<div class="span3 f_error">
						<label>Error state</label>
						<input type="text" class="span10">
						<span class="help-block">help block</span>
					</div>
					<div class="span3 f_warning">
						<label>Warning state</label>
						<input type="text" class="span10">
						<span class="help-block">help block</span>
					</div>
					<div class="span3 f_success">
						<label>Success state</label>
						<input type="text" class="span10">
						<span class="help-block">help block</span>
					</div>
				</div>
				<div class="row-fluid">
					<div class="span3">
						<div class="input-prepend input-append">
							<span class="add-on">$</span><input type="text" size="16" class="span5 focused"><span class="add-on">.00</span>
						</div>
					</div>
					<div class="span3 f_error">
						<div class="input-prepend input-append">
							<span class="add-on">$</span><input type="text" size="16" class="span5"><span class="add-on">.00</span>
						</div>
					</div>
					<div class="span3 f_warning">
						<div class="input-prepend input-append">
							<span class="add-on">$</span><input type="text" size="16" class="span5"><span class="add-on">.00</span>
						</div>
					</div>
					<div class="span3 f_success">
						<div class="input-prepend input-append">
							<span class="add-on">$</span><input type="text" size="16" class="span5"><span class="add-on">.00</span>
						</div>
					</div>
				</div>
				<div class="row-fluid">
					<div class="span3">
					   <label>Disabled</label>
					   <input type="text" disabled="disabled" class="span10">
					</div>
				</div>
			</div>
			<div class="formSep">
				<label>Form elements with <code>.span*</code> classes</label>
				<div class="row-fluid sepH_b stacked">
					<div class="span6">
						<input type="text" class="span3" placeholder=".span3">
						<input type="text" class="span5" placeholder=".span5">
						<input type="text" class="span7" placeholder=".span7">
					</div>
					<div class="span6">
						<select class="span3">
							<option>1</option>
							<option>2</option>
							<option selected="selected">3</option>
							<option>4</option>
							<option>5</option>
						</select>
						<select class="span5">
							<option>1</option>
							<option>2</option>
							<option>3</option>
							<option>4</option>
							<option selected="selected">5</option>
						</select>
						<select class="span7">
							<option>1</option>
							<option>2</option>
							<option>3</option>
							<option>4</option>
							<option>5</option>
							<option>6</option>
							<option selected="selected">7</option>
						</select>
					</div>
				</div>
				<label>Alternate static sizes</label>
				<div class="row-fluid">
					<div class="span3">
						<input type="text" class="input-mini" placeholder=".input-mini">
					</div>
					<div class="span3">
						<input type="text" class="input-small" placeholder=".input-small">
					</div>
					<div class="span3">
						<input type="text" class="input-medium" placeholder=".input-medium">
					</div>
				</div>
			</div>
		</form>
        
		<div class="row-fluid">
			<div class="span12">
				<div class="formSep">
					<label class="sepH_b">Progress bars</label>
					<div class="row-fluid">
						<div class="span3">
							<div class="sepH_b progress progress-info">
								<div style="width: 20%" class="bar"></div>
							</div>
						</div>
						<div class="span3">
							<div class="sepH_b progress progress-success">
								<div style="width: 40%" class="bar"></div>
							</div>
						</div>
						<div class="span3">
							<div class="sepH_b progress progress-warning">
								<div style="width: 60%" class="bar"></div>
							</div>
						</div>
						<div class="span3">
							<div class="sepH_b progress progress-danger">
								<div style="width: 80%" class="bar"></div>
							</div>
						</div>
					</div>
				</div>
				<div class="formSep">
					<label class="sepH_b">Stripped progress bars</label>
					<div class="row-fluid">
						<div class="span3">
							<div class="sepH_b progress progress-info progress-striped">
								<div style="width: 14%" class="bar"></div>
							</div>
						</div>
						<div class="span3">
							<div class="sepH_b progress progress-success progress-striped active">
								<div style="width: 66%" class="bar"></div>
							</div>
						</div>
						<div class="span3">
							<div class="sepH_b progress progress-warning progress-striped">
								<div style="width: 42%" class="bar"></div>
							</div>
						</div>
						<div class="span3">
							<div class="sepH_b progress progress-danger progress-striped active">
								<div style="width: 88%" class="bar"></div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
        <div class="row-fluid">
            <div class="span6">
				<form class="form-horizontal well">
					<fieldset>
						<p class="f_legend">Horizontal form</p>
						<div class="control-group">
							<label class="control-label">Text input</label>
							<div class="controls">
								<input type="text" class="span10">
								<span class="help-block">block help text</span>
							</div>
						</div>
						<div class="control-group">
							<label class="control-label">Text input</label>
							<div class="controls">
								<textarea cols="30" rows="5" class="span10"></textarea>
								<span class="help-block">block help text</span>
							</div>
						</div>
						<div class="control-group">
							<div class="controls">
								<button class="btn" type="submit">Form button</button>
							</div>
						</div>
					</fieldset>
				</form>
            </div>
            <div class="span6">
				<form class="well form-inline">
					<p class="f_legend">Inline form</p>
					<input type="text" placeholder="Email" class="input-small">
					<input type="password" placeholder="Password" class="input-small">
					<button class="btn" type="submit">Sign in</button>
				</form>
            </div>
        </div>
    </div>
</div>