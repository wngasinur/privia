<script type="text/html" id='latestTransactionSection'>
<tr>
                        <td class="latest"> 
                        <div class="vcard">
                        <img alt="" src="<%=ditambahOleh.imgProfile%>" class="thumbnail" />
                            <div class="vcard-label">
                            <label><%=ditambahOleh.nama%></label>
                            </div>
                        </div>
                        <div class="info">
                            <span class="badge">Proposal</span>
                            <small>Telah mengajukan proposal pinjaman selama <%=lamaPinjaman%> tahun kepada <b><%=namaCustomer%></b> dengan total pinjaman <%=cashNCarry.formatPrice()%> , dan pembayaran bulanan <%=payPerMonth.formatPrice()%>
 <blockquote><%=moment(tglTambah).fromNow()%></blockquote>
                        </small>
                        </div>

                        </td>
                       
                    </tr>
</script>

